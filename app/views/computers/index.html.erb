<div class = "comp-list">
<br>
<ul>
    <% @computers.each do |computer| %>
      <li>
          <%= computer.name %>
          |
          <%= link_to 'Assess', assessments_path %>
          |
          <%= link_to 'Edit', edit_computer_path(1) %>
          |
          <%= link_to 'Remove', method: :delete, data: { confirm: 'Are you sure?' } %>
      </li>
    <% end %>
</ul>
<br>
</div>
<br>
<h4><%=link_to "Add a Computer", new_computer_path %></h4>
